{
  "containers": {
    "llamafiler": {
      "image": "llamafiler-embeddings:latest"
    },
    "nginx": {
      "image": "llamafiler-embeddings-nginx:latest",
      "ports": {
        "80": "HTTP"
      },
      "environment": {
        "AUTH_USERNAME": "zauru",
        "AUTH_PASSWORD": "intuitiva2015"
      }
    }
  },
  "publicEndpoint": {
    "containerName": "nginx",
    "containerPort": 80,
    "healthCheck": {
      "healthyThreshold": 2,
      "unhealthyThreshold": 2,
      "timeoutSeconds": 5,
      "intervalSeconds": 30,
      "path": "/health"
    }
  }
} 